<div id="<%= dom_id type_of_quiz %>">
  <tr>
    <td class="px-6 py-4 whitespace-nowrap text-sm font-medium text-gray-900">
      <%= image_tag "type_of_quiz/mcq.png",class: "h-34" %>
    </td>
    <td class="px-6 py-4 whitespace-nowrap text-sm font-medium text-gray-900">
      <%= type_of_quiz.name %>
    </td>
    <td class="px-6 py-4 whitespace-nowrap text-sm font-medium text-gray-900">
      <%= type_of_quiz.description %>
    </td>
    <td class="px-6 py-4 whitespace-nowrap text-right text-sm font-medium">
      <% if user_signed_in? && current_user.role != "admin" %>
      <%= link_to "Edit Type Of Quiz",edit_type_of_quiz_path(type_of_quiz),class:"inline-flex items-center px-4 py-2 border border-transparent shadow-sm text-sm font-medium rounded-md text-white bg-green-600 hover:bg-green-700 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-green-500"%>      
      <% end %>
      <%= link_to "Show Quiz",questions_path(type_of_quiz: type_of_quiz.id),class:"inline-flex items-center px-4 py-2 border border-transparent shadow-sm text-sm font-medium rounded-md text-white bg-green-600 hover:bg-green-700 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-green-500"%>
    </td>
  </tr>
</div>
