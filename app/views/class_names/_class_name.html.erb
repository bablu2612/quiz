<div id="<%= dom_id class_name %>">
  <tr>
    <td class="px-6 py-4 whitespace-nowrap text-sm font-medium text-gray-900">
      <%= class_name.name %>
    </td>
    <td class="px-6 py-4 whitespace-nowrap text-sm font-medium text-gray-900">
      <%=  %>
    </td>
    <td class="px-6 py-4 whitespace-nowrap text-right text-sm font-medium">
        <%= link_to "Edit",edit_class_name_path(class_name),class:"inline-flex items-center px-4 py-2 border border-transparent shadow-sm text-sm font-medium rounded-md text-white bg-green-600 hover:bg-green-700 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-green-500"%>
        <%= link_to "Details",class_name_module_names_path(class_name.id),class:"inline-flex items-center px-4 py-2 border border-transparent shadow-sm text-sm font-medium rounded-md text-white bg-green-600 hover:bg-green-700 focus:outline-none focus:ring-2 focus:ring-offset-2 focus:ring-green-500"%>
        <%= link_to 'Delete',class_name,method: :delete,data: {confirm: "Are you sure all Modules,Levels,Quizzes under this class will also be deleted"} ,class: "inline-flex items-center bg-transparent hover:bg-red-500 text-red-600 font-semibold hover:text-white py-2 px-4 border border-red-500 hover:border-transparent rounded" %>    
    </td>
  </tr>
</div>
